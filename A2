#include <bits/stdc++.h> 
using namespace std; 

int main() 
{ 
	string a = "Geeks"; 
	string b = "function"; 
	cout << "Value of a before: " << a << endl; 
	cout << "Value of b before: " << b << endl; 
	swap(a, b); 
	cout << "Value of a now: " << a << endl; 
	cout << "Value of b now: " << b << endl; 
	cout << "values are now defined";
	return 0; 
} 
